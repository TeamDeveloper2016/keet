<?xml version="1.0" encoding="ISO-8859-1"?>
<!--
    Relación de consultas para las tablas de SAKBE
-->
<process>
  <model>    
  </model>

  <dml>
    <unit id="TcSakbeTiposMaquinariasDto">
      <select id="row">
        select      
          *
        from      
          tc_sakbe_tipos_maquinarias
        where 
          {condicion} 
      </select>
      <select id="identically">
        select      
          *
        from      
          tc_sakbe_tipos_maquinarias
        where      
          clave = '{clave}'
      </select>
    </unit>    
