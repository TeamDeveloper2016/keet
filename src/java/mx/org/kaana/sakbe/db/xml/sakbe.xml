          id_suministro= {idSuministro}
      </select>      
			<select id="siguiente">
        select
					max(orden)+ 1 as siguiente
				from 
					tc_sakbe_suministros
				where				  
				  ejercicio = {ejercicio}			
			</select>			
    </unit>
