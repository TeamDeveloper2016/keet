		 <clase expresion="0 0 2 1/1 * ? *"    id="respaldos"   description="respaldar BD"            load="true"  pruebas="*" produccion="*" >mx.org.kaana.jobs.Respaldos</clase>
		 <clase expresion="0 0 5 1/1 * ? *"    id="reiniciar"   description="reinciar servidor"       load="true"  pruebas="*" produccion="*" >mx.org.kaana.jobs.Reiniciar</clase>
		 <clase expresion="0 0/30 * 1/1 * ? *" id="facturas"    description="sincronización facturas" load="false" pruebas="*" produccion="*" >mx.org.kaana.jobs.Facturas</clase>
		 <clase expresion="0 0 7,14 ? * * *"   id="descargas"   description="descargas de respaldos"  load="false" pruebas="*" produccion="*" >mx.org.kaana.jobs.Descargas</clase>
		 <clase expresion="0 0,30 0/1 ? * * *" id="facturacion" description="facturación automatica"  load="false" pruebas="*" produccion="*" >mx.org.kaana.jobs.Timbrado</clase>		 
		 <clase expresion="0 0 4 1/1 * ? *"    id="depuracion"  description="depuración de archivos"  load="true"  pruebas="*" produccion="*" >mx.org.kaana.jobs.Depuracion</clase>		 
		 <clase expresion="0 0 8 1/1 * ? *"    id="garantias"   description="fondos de garantias"     load="false" pruebas="*" produccion="*" >mx.org.kaana.jobs.Garantias</clase>		 
		 <clase expresion="0 0 17 ? * FRI *"   id="balance"     description="estados de cuenta"       load="false" pruebas="*" produccion="*" >mx.org.kaana.jobs.Balance</clase>		 
		 <clase expresion="0 0 8 ? * SAT *"    id="cierre"      description="cierre de nómina gilvy"  load="true"  pruebas="*" produccion="*" >mx.org.kaana.jobs.Cierre</clase>		 
		 <clase expresion="0 00 10 ? * THU *"  id="corrida"     description="corrida de nómina cafu"  load="true"  pruebas="*" produccion="*" >mx.org.kaana.jobs.Apertura</clase>		 
		 <clase expresion="0 30 15 ? * THU *"  id="anticipada"  description="cierre de nómina cafu"   load="true"  pruebas="*" produccion="*" >mx.org.kaana.jobs.Cierre</clase>		 
		 <clase expresion="0 0 6 1/1 * ? *"    id="contratos"   description="activar contratos"       load="false"  pruebas="*" produccion="*" >mx.org.kaana.jobs.Contratos</clase>
   </tarea>      	        
</tareas>
